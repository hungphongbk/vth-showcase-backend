{
  "name": "graphql-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist && rm -rf src/schema.gql",
    "prestart:dev": "rimraf dist && rm -rf src/schema.gql",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "DB_PORT=5433 nest start main",
    "start:dev": "DB_PORT=5433 nest start main --watch",
    "start:debug": "DB_PORT=5433 nest start main --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/main/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/main/src/ormconfig.ts",
    "mig": "npm run typeorm migration:run",
    "mig:create": "npm run typeorm migration:create -- -n",
    "mig:generate": "npm run typeorm migration:generate -- -n",
    "mig:revert": "npm run typeorm migration:revert",
    "typeorm-seeding": "ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName src/ormconfig.ts",
    "seed:config": "npm run typeorm-seeding config",
    "seed:run": "npm run typeorm-seeding seed",
    "schema:drop": "npm run typeorm schema:drop",
    "schema:sync": "npm run typeorm schema:sync",
    "init:dev": "npm run schema:drop && npm run schema:sync && npm run seed:run"
  },
  "dependencies": {
    "@graphql-tools/utils": "^7.10.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs-query/query-graphql": "^0.30.0",
    "@nestjs-query/query-typeorm": "^0.30.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.3",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/platform-fastify": "^8.2.3",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/terminus": "^8.0.3",
    "@nestjs/typeorm": "^8.0.2",
    "@tfarras/nestjs-firebase-admin": "^2.0.1",
    "@tfarras/nestjs-firebase-auth": "^2.0.0",
    "@types/lodash": "^4.14.177",
    "apollo-server-cache-redis": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "apollo-server-fastify": "^3.5.0",
    "apollo-server-plugin-base": "^3.4.0",
    "apollo-server-plugin-response-cache": "^3.4.0",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "dataloader": "^2.0.0",
    "date-fns": "^2.27.0",
    "faker": "^5.5.3",
    "firebase-admin": "^9.12.0",
    "form-data": "^4.0.0",
    "glob": "^7.2.0",
    "graphql": "^15.7.2",
    "graphql-scalars": "^1.13.6",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^12.0.0",
    "ioredis": "^4.28.2",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "node-fetch": "^2.6.6",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.2",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41",
    "typeorm-extension": "^1.0.1",
    "typeorm-seeding": "^1.6.1",
    "url-join": "^4.0.1",
    "url-join-ts": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^3.0.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/url-join": "^4.0.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/upload(|/.*)$": "<rootDir>/libs/upload/src/$1"
    }
  }
}
