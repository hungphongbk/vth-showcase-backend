{
  "name": "graphql-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "cron": "dist/apps/cron/main.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist && rm -rf src/schema.gql",
    "prestart:dev": "rimraf dist && rm -rf src/schema.gql",
    "build": "concurrently npm:build:*",
    "build:main": "nest build main",
    "build:cron-pub": "nest build cron-pub",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start main",
    "start:dev": "env-cmd -f .env.development nest start main --watch",
    "start:debug": "nest start main --watch --debug",
    "start:cron-pub": "nest start cron-pub",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/main/test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "mig": "npm run typeorm migration:run",
    "mig:create": "typeorm-ts-node-commonjs migration:create -- -n",
    "mig:revert": "npm run typeorm migration:revert",
    "typeorm-seeding": "ts-node ./node_modules/typeorm-seeding/dist/cli.js --dataSource src/ormconfig.ts --pretty",
    "seed:config": "npm run typeorm-seeding config",
    "seed:run": "npm run typeorm-seeding seed",
    "schema:drop": "npm run typeorm schema:drop",
    "schema:sync": "npm run typeorm schema:sync",
    "init:dev": "npm run schema:drop && npm run schema:sync && npm run seed:run"
  },
  "dependencies": {
    "@google-analytics/data": "2.8.0",
    "@graphql-tools/utils": "8.12.0",
    "@hungphongbk/nestjs-firebase-admin": "^2.0.1",
    "@hungphongbk/nestjs-firebase-auth": "^2.0.1",
    "@nestjs-query/core": "0.30.0",
    "@nestjs-query/query-graphql": "0.30.0",
    "@nestjs-query/query-typeorm": "0.30.0",
    "@nestjs/apollo": "10.1.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/graphql": "10.1.1",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/platform-fastify": "8.4.7",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/terminus": "8.1.1",
    "@nestjs/typeorm": "9.0.1",
    "@ntegral/nestjs-sentry": "3.0.7",
    "@sentry/integrations": "6.17.6",
    "@sentry/node": "6.17.6",
    "@sentry/tracing": "6.17.6",
    "@types/lodash": "4.14.185",
    "@types/passport": "1.0.11",
    "amqp-connection-manager": "4.1.6",
    "amqplib": "0.10.3",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-fastify": "3.10.2",
    "apollo-server-plugin-base": "3.6.2",
    "apollo-server-plugin-response-cache": "3.7.0",
    "cache-manager": "3.6.3",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "crypto": "1.0.1",
    "dataloader": "2.1.0",
    "date-fns": "2.28.0",
    "faker": "5.5.3",
    "firebase-admin": "10.3.0",
    "form-data": "4.0.0",
    "generate-password": "1.7.0",
    "graphql": "15.8.0",
    "graphql-scalars": "1.13.6",
    "graphql-subscriptions": "1.2.1",
    "graphql-type-json": "0.3.2",
    "graphql-upload": "12.0.0",
    "ioredis": "4.28.5",
    "joi": "17.6.0",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "nest-commander": "2.5.0",
    "node-fetch": "2.6.7",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "slugify": "1.6.5",
    "typeorm": "0.3.9",
    "typeorm-extension": "2.1.10",
    "typeorm-seeding": "1.6.1",
    "url-join": "4.0.1",
    "url-join-ts": "1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.3",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@sentry/webpack-plugin": "1.19.0",
    "@types/cache-manager": "3.4.3",
    "@types/cron": "1.7.3",
    "@types/date-fns": "2.6.0",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.58",
    "@types/node-fetch": "3.0.3",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.11",
    "@types/url-join": "4.0.1",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "concurrently": "7.0.0",
    "dotenv-cli": "4.1.1",
    "env-cmd": "10.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "7.2.3",
    "glob-promise": "4.2.2",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "source-map-support": "0.5.21",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "3.14.1",
    "typescript": "<4.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/upload(|/.*)$": "<rootDir>/libs/upload/src/$1",
      "^@app/mailer(|/.*)$": "<rootDir>/libs/mailer/src/$1",
      "^@app/firebase(|/.*)$": "<rootDir>/libs/firebase/src/$1",
      "^@app/query-firestore(|/.*)$": "<rootDir>/libs/query-firestore/src/$1",
      "^@app/investor(|/.*)$": "<rootDir>/libs/investor/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@app/fcm(|/.*)$": "<rootDir>/libs/fcm/src/$1",
      "^@app/rabbitmq(|/.*)$": "<rootDir>/libs/rabbitmq/src/$1",
      "^@app/configs(|/.*)$": "<rootDir>/libs/configs/src/$1",
      "^@app/ga-data(|/.*)$": "<rootDir>/libs/ga-data/src/$1",
      "^@app/sentry-logger(|/.*)$": "<rootDir>/libs/sentry-logger/src/$1",
      "^@app/util(|/.*)$": "<rootDir>/libs/util/src/$1"
    }
  }
}
